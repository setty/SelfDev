    <labels>
        <fullName>RKRSSNFR_SearchCleanserRegex</fullName>
        <categories>RKRSSNFR, regex, search cleanser</categories>
        <language>en_US</language>
        <protected>false</protected>
        <shortDescription>Regex to remove unwanted words from Search value for Google News Feed</shortDescription>
        <value>(\s*(\W|\b(and|ag|associates|bank|bv|company|co|corp|division|fcu|gmbh|group|holdings|inc|industries|limited|llc|lp|ltd|ltd.|nv|partners|plc|private|public limited company|pvt|pvt.|solutions|sons|son|sa|technologies|trust)\W?))*$</value>
    </labels>
