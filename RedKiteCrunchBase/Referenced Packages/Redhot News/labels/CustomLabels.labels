    <labels>
        <fullName>RKRSSNFR_HtmlTagRemoverRegex</fullName>
        <categories>RKRSSNFR, regex, html tag remover</categories>
        <language>en_US</language>
        <protected>false</protected>
        <shortDescription>Regex to remove the Html tags from Google RSS News Feed article descriptions</shortDescription>
        <value>&lt;/?\w+((\s+\w+(\s*=\s*(?:&quot;.*?&quot;|&apos;.*?&apos;|[^&apos;&quot;&gt;\s]+))?)+\s*|\s*)/?&gt;</value>
    </labels>
