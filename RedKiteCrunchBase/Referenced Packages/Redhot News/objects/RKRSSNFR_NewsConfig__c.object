<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <customSettingsVisibility>Public</customSettingsVisibility>
    <description>Stores and maintains the configuration parameters for the RK News App</description>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>GoogleNewsEdition__c</fullName>
        <defaultValue>&quot;us&quot;</defaultValue>
        <deprecated>false</deprecated>
        <description>Tells the News Search system which edition of news to pull results from. For example, &quot;us&quot; specifies the US edition</description>
        <externalId>false</externalId>
        <inlineHelpText>Tells the News Search system which edition of news to pull results from. For example, &quot;us&quot; specifies the US edition</inlineHelpText>
        <label>Google News Edition</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>InputEncoding__c</fullName>
        <defaultValue>&quot;UTF-8&quot;</defaultValue>
        <deprecated>false</deprecated>
        <description>Encoding format used to send input to Google News search. E.g. &quot;UTF-8&quot;</description>
        <externalId>false</externalId>
        <inlineHelpText>Encoding format used to send input to Google News search. E.g. &quot;UTF-8&quot;</inlineHelpText>
        <label>Input Encoding Format</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>OrderScore__c</fullName>
        <defaultValue>&quot;r&quot;</defaultValue>
        <deprecated>false</deprecated>
        <description>The mechanism by which Google News output messages are ordered. Possible values are as follows:
 r - relevance, 
d - newest to oldest with duplicates included, 
n - newest to oldest, 
o - oldest to newest.</description>
        <externalId>false</externalId>
        <inlineHelpText>The mechanism by which Google News output messages are ordered. Possible values are as follows:
 r - relevance, 
d - newest to oldest with duplicates included, 
n - newest to oldest, 
o - oldest to newest.</inlineHelpText>
        <label>Order Search Results By</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>OutputEncoding__c</fullName>
        <defaultValue>&quot;UTF-8&quot;</defaultValue>
        <deprecated>false</deprecated>
        <description>Encoding format used for the response received from Google News search .e.g. &quot;UTF-8&quot;</description>
        <externalId>false</externalId>
        <inlineHelpText>Encoding format used for the response received from Google News search .e.g. &quot;UTF-8&quot;</inlineHelpText>
        <label>Output Encoding Format</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>OutputFormat__c</fullName>
        <defaultValue>&quot;rss&quot;</defaultValue>
        <deprecated>false</deprecated>
        <description>The format in which the response is required from Google News Search. Possible values are &quot;rss&quot; or &quot;atom&quot;.</description>
        <externalId>false</externalId>
        <inlineHelpText>The format in which the response is required from Google News Search. Possible values are &quot;rss&quot; or &quot;atom&quot;.</inlineHelpText>
        <label>OutputFormat</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>PreferredLanguageForInterface__c</fullName>
        <defaultValue>&quot;en&quot;</defaultValue>
        <deprecated>false</deprecated>
        <description>Language preference for the interface. Eg: &quot;en&quot;.</description>
        <externalId>false</externalId>
        <inlineHelpText>Language preference for the interface. Eg: &quot;en&quot;.</inlineHelpText>
        <label>Preferred Language for Interface</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>PreferredLanguageForResults__c</fullName>
        <defaultValue>&quot;en&quot;</defaultValue>
        <deprecated>false</deprecated>
        <description>Language preference of the Google News Search results. Eg: &quot;en&quot;</description>
        <externalId>false</externalId>
        <inlineHelpText>Language preference of the Google News Search results. Eg: &quot;en&quot;</inlineHelpText>
        <label>Preferred Language for Results</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>SearchResultCount__c</fullName>
        <defaultValue>&quot;10&quot;</defaultValue>
        <deprecated>false</deprecated>
        <description>Number of Google News search results to be returned. e.g.: 10</description>
        <externalId>false</externalId>
        <inlineHelpText>Number of Google News search results to be returned. e.g.: 10</inlineHelpText>
        <label>Count of Search Results</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>ShareArticleUsingChatter__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <description>Allows  to configure whether the Chatter button on the News Article Feed page is enabled or disabled.</description>
        <externalId>false</externalId>
        <inlineHelpText>Allows  to configure whether the Chatter button on the News Article Feed page is enabled or disabled.</inlineHelpText>
        <label>Share Article Using Chatter</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>ShareArticleUsingLinkedIn__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <description>Allows the user to configure whether the LinkedIn button on the News Article Feed page is enabled or disabled.</description>
        <externalId>false</externalId>
        <inlineHelpText>Allows the user to configure whether the LinkedIn button on the News Article Feed page is enabled or disabled.</inlineHelpText>
        <label>Share Article Using LinkedIn</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>ShareArticleUsingTwitter__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <description>Allows the user to configure whether the Twitter button on the News Article Feed page is enabled or disabled.</description>
        <externalId>false</externalId>
        <inlineHelpText>Allows the user to configure whether the Twitter button on the News Article Feed page is enabled or disabled.</inlineHelpText>
        <label>Share Article Using Twitter</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Share_Article_Using_Facebook__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>Allows the user to configure whether the Twitter button on the News Article Feed page is enabled or disabled.</description>
        <externalId>false</externalId>
        <inlineHelpText>Allows the user to configure whether the Twitter button on the News Article Feed page is enabled or disabled.</inlineHelpText>
        <label>Share Article Using Facebook</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Share_Using_SalesForce_Email__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <description>Allows the user to configure whether the Share Using SalesForce Email button on the News Article Feed page is enabled or disabled. If the Security Setting restrict the user from sending emails, then hiding this button would automatically enable the &quot;Send Email Using Mail Client&quot; button which would allow the user to share the article through email using mail client (ex. MS Outlook) installed on the local machine.</description>
        <externalId>false</externalId>
        <inlineHelpText>Hides or displays the Share Using SalesForce Email button. If unchecked automatically activates the &quot;Send Email Using Mail Client&quot; button which would allow the user to share the article using mail client (ex. MS Outlook) installed on the local machine.</inlineHelpText>
        <label>Share Using SalesForce Email</label>
        <type>Checkbox</type>
    </fields>
    <label>News Feed Configuration</label>
</CustomObject>
