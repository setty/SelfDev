    <fields>
        <fullName>Age_Years__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(NOT(ISNULL(Date_of_Birth__c)),
     ROUND((TODAY()-Date_of_Birth__c) / 365.2425, 2),
     0
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Age (Years)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>1</scale>
        <trackHistory>false</trackHistory>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Age__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(
    NOT(ISNULL(Date_of_Birth__c)),
    IF(TODAY()-Date_of_Birth__c &lt; 365.2425,
         IF(TODAY()-Date_of_Birth__c &lt; 31,
              TEXT(TODAY()-Date_of_Birth__c) &amp; &quot;d&quot;,
              TEXT(FLOOR((TODAY()-Date_of_Birth__c) / 30)) &amp; &quot;m&quot;),
         TEXT(FLOOR((TODAY()-Date_of_Birth__c) / 365.2425)) &amp; &quot;y&quot;
        ),
    &apos;Unknown&apos;
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Age</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
    </fields>
