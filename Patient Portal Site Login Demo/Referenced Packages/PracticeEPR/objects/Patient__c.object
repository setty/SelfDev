    <fields>
        <fullName>Ideal_Weight__c</fullName>
        <deprecated>false</deprecated>
        <description>Ideal Weight for Men &amp; Women based on Devine Formula.</description>
        <externalId>false</externalId>
        <formula>IF (
ISPICKVAL( Gender__c , &apos;Male&apos;),
IF(Height__c &gt; 88, 50 + ( ( (  Height__c  - 152.4 ) * 0.3937008 ) * 2.3 ), 0) ,
IF(Height__c &gt; 93, 45.5 + ( ( ( Height__c - 152.4 ) * 0.3937008 ) * 2.3 ), 0)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Ideal Weight for Men &amp; Women based on Devine Formula</inlineHelpText>
        <label>Ideal Weight (kg)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <type>Number</type>
        <unique>false</unique>
    </fields>
