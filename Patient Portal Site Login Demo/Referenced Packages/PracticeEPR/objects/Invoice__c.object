    <deploymentStatus>InDevelopment</deploymentStatus>
    <deprecated>false</deprecated>
    <description>Invoice for a designated bill payer. This can be automatically generated or manually created by a user, normally  in the case of providing credit notes.</description>
    <enableActivities>false</enableActivities>
    <enableFeeds>false</enableFeeds>
    <enableHistory>false</enableHistory>
    <enableReports>true</enableReports>
    <fields>
        <fullName>Account2__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <label>Account</label>
        <referenceTo>Account</referenceTo>
        <relationshipLabel>Invoices</relationshipLabel>
        <relationshipName>Invoices1</relationshipName>
        <required>false</required>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>BP_Address__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>Bill_Payer__r.Name  + &apos;, &apos; +  Bill_Payer__r.Billing_Address__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>BP Address</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Balance__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <label>Balance</label>
        <precision>14</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Bill_Payer__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <label>Bill Payer</label>
        <referenceTo>Bill_Payer__c</referenceTo>
        <relationshipLabel>Invoices</relationshipLabel>
        <relationshipName>Invoices</relationshipName>
        <relationshipOrder>0</relationshipOrder>
        <type>MasterDetail</type>
        <writeRequiresMasterRead>true</writeRequiresMasterRead>
    </fields>
    <fields>
        <fullName>Due_Date_DDMMYYYY__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>TEXT(DAY( Due_Date__c )) &amp; &apos;/&apos; &amp; LPAD(TEXT(MONTH(Due_Date__c )), 2, &apos;0&apos;) &amp; &apos;/&apos; &amp; TEXT(YEAR(Due_Date__c ))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Due Date (DD/MM/YYYY)</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Due_Date__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>Invoice_Date__c +  
IF(NOT(ISNULL( Bill_Payer__r.Payment_Days__c )), 
     Bill_Payer__r.Payment_Days__c,  
    $Setup.Billing_Parameters__c.Payment_Days__c 
  )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Due Date</label>
        <required>false</required>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Final_Diagnosis__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <label>Final Diagnosis</label>
        <length>30</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Initial_Diagnosis__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <label>Initial Diagnosis</label>
        <length>30</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Inv_Date_DDMMYYYY__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>TEXT(DAY( Invoice_Date__c )) &amp; &apos;/&apos; &amp; LPAD(TEXT(MONTH(Invoice_Date__c )), 2, &apos;0&apos;) &amp; &apos;/&apos; &amp; TEXT(YEAR(Invoice_Date__c ))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Inv Date (DD/MM/YYYY)</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Invoice_Amount__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <label>Invoice Amount</label>
        <summarizedField>Invoice_Item__c.Price__c</summarizedField>
        <summaryForeignKey>Invoice_Item__c.Invoice__c</summaryForeignKey>
        <summaryOperation>sum</summaryOperation>
        <type>Summary</type>
    </fields>
    <fields>
        <fullName>Invoice_Date__c</fullName>
        <defaultValue>$System.OriginDateTime</defaultValue>
        <deprecated>false</deprecated>
        <description>Date invoice generated</description>
        <externalId>false</externalId>
        <label>Invoice Date</label>
        <required>false</required>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>Payment_Received__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>Enter the amount received for this invoice</inlineHelpText>
        <label>Payment Received</label>
        <precision>14</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Status__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <label>Status</label>
        <picklist>
            <picklistValues>
                <fullName>Invoiced</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Overdue</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Paid</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Written Off</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Tax__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>CEILING(100*Invoice_Amount__c *  $Setup.Billing_Parameters__c.Tax_Rate__c)/100</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Tax</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Total_Amount__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>Invoice_Amount__c +  Tax__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Total Amount</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Transaction_Type__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <label>Transaction Type</label>
        <picklist>
            <picklistValues>
                <fullName>Invoice</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Credit Note</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <type>Picklist</type>
    </fields>
