        <externalId>false</externalId>
        <label>SysApptIndex</label>
        <precision>3</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Treatment__c</fullName>
        <deprecated>false</deprecated>
        <description>If appointment has been created from a treatment the id of the treatment to which this relates. It is not intended that users should &apos;lookup&apos; a treatment for every appointment.</description>
        <externalId>false</externalId>
        <label>Treatment</label>
        <referenceTo>Treatment__c</referenceTo>
        <relationshipLabel>Appointments</relationshipLabel>
        <relationshipName>Appointments</relationshipName>
        <required>false</required>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Triage__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <label>Triage</label>
        <picklist>
            <picklistValues>
                <fullName>1. Critical</fullName>
                <color>#FF0000</color>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>2. Urgent</fullName>
                <color>#FF9900</color>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>3. Severe</fullName>
                <color>#33CC00</color>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>4. Minor</fullName>
                <color>#33CCFF</color>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Type__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <label>Type</label>
        <picklist>
            <picklistValues>
                <fullName>A&amp;E</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Appointment</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Follow Up</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Walk In</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <type>Picklist</type>
    </fields>
    <label>Appointment</label>
