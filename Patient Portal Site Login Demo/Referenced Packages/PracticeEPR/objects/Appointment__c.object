    <deploymentStatus>InDevelopment</deploymentStatus>
    <deprecated>false</deprecated>
    <description>A patient appointment for scheduling with a clinician&apos;s diary</description>
    <enableActivities>false</enableActivities>
    <enableFeeds>false</enableFeeds>
    <enableHistory>false</enableHistory>
    <enableReports>true</enableReports>
    <fields>
        <fullName>Appointment_Date_Time__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <label>Appointment Date/Time</label>
        <required>false</required>
        <type>DateTime</type>
    </fields>
    <fields>
        <fullName>Chargeable__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <label>Chargeable?</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Description__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>Patient__r.Name + &apos; &apos; +   Treatment__r.Name</formula>
        <label>Description</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Duration__c</fullName>
        <defaultValue>30</defaultValue>
        <deprecated>false</deprecated>
        <description>Duration of appointment in minutes</description>
        <externalId>false</externalId>
        <inlineHelpText>Enter duration of appointment in minutes</inlineHelpText>
        <label>Duration</label>
        <precision>6</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>EmailSMSProvider__c</fullName>
        <defaultValue>$Setup.Default__c.Email_To_SMS_Provider__c</defaultValue>
        <deprecated>false</deprecated>
        <description>System field</description>
        <externalId>false</externalId>
        <label>EmailSMSProvider</label>
        <required>false</required>
        <type>Email</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>End_Dtm__c</fullName>
        <deprecated>false</deprecated>
        <description>End time for the appointment for display in lists</description>
        <externalId>false</externalId>
        <formula>Appointment_Date_Time__c +  (Duration__c / 1440)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Appointment End Time</label>
        <required>false</required>
        <type>DateTime</type>
    </fields>
    <fields>
        <fullName>EventId__c</fullName>
        <deprecated>false</deprecated>
        <description>Internal system field for maintaining Event and Appointment synchronisation</description>
        <externalId>false</externalId>
        <label>EventId</label>
        <length>18</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>PatientMobile__c</fullName>
        <deprecated>false</deprecated>
        <description>Patient mobile number for SMS reminders</description>
        <externalId>false</externalId>
        <formula>IF( Patient__r.Receive_SMS__c, Patient__r.Patient_Mobile__c, &apos;&apos;)</formula>
        <label>PatientMobile</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Patient_Email__c</fullName>
        <deprecated>false</deprecated>
        <description>Email address for patient reminder, copied automatically from the current patient email address field (if populated)</description>
        <externalId>false</externalId>
        <label>Patient Email</label>
        <required>false</required>
        <type>Email</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Patient__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <label>Patient</label>
