    <fields>
        <fullName>BMI__c</fullName>
        <deprecated>false</deprecated>
        <description>Body Mass Index kg/m^2</description>
        <externalId>false</externalId>
        <formula>IF (Height_cm__c &gt; 0, 
     Weight__c /  ((Height_cm__c / 100)^2),
     0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>kg/m^2</inlineHelpText>
        <label>BMI</label>
        <precision>18</precision>
        <required>false</required>
        <scale>1</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
