        <fullName>Ideal_Weight_kg__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF ( 
 ISPICKVAL( Patient__r.Gender__c ,  &apos;Male&apos;), 
 50 + ( ( ( Height_cm__c - 152.4 ) * 0.3937008 ) * 2.3 ) , 
 45.5 + ( ( ( Height_cm__c - 152.4 ) * 0.3937008 ) * 2.3 )
 )</formula>
