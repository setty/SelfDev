        <fullName>Item_Date__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF( NOT( ISBLANK( Lab_Test__c ) ) ,
     Lab_Test__r.CreatedDate,
     IF( NOT( ISBLANK( Prescription__c ) ) ,
          Prescription__r.LastModifiedDate,
          Treatment__r.LastModifiedDate
     )
)</formula>
        <label>Date</label>
        <required>false</required>
        <type>Date</type>
    </fields>
